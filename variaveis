#include <iostream>

using namespace std;

int main()
{
	string characterName = "Andrei";
	int characterAge;
	characterAge = 20;
    cout << "Era uma vez um homem chamado " << characterName << end1;
    cout << "ele tinha 20 anos" << end1;
    cout << "ele gosta do nome " << characterName << endl;
    cout << "mas nÃ£o gostava de ter " << characterAge << end1;
    return 0;
}
